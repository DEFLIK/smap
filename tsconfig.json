{
  "include": [
    "./src-front/*"
  ],
  "compilerOptions": {
    "target": "ES5", 
    "lib": [
      "ES2020",
      "ES2020.Promise",
      "DOM"
    ],
    "module": "AMD", 
    "outDir": "./static/scripts",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "noImplicitAny": false
  }
}
